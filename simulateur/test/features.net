INPUT e
OUTPUT slice1, concat, select1, x, o, a, n
VAR
slice1 : 2, slice2 : 2, select1, select2, x, o, a, n, concat : 4, e : 4
IN

concat = CONCAT slice1 select1
slice1 = SLICE 2 3 e
slice2 = SLICE 0 1 e
select1 = SELECT 0 e
select2 = SELECT 1 e

a = AND select1 select2
x = XOR select1 select2
o = OR select1 select2
n = NOT a





